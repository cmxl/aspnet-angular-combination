    <meta name="apple-mobile-web-app-title" content="NooBIT" />
    <meta name="application-name" content="NooBIT" />
    <meta name="msapplication-TileColor" content="#2b5797" />
    <meta name="msapplication-TileImage" content="https://www.noobit.dev/assets/icons/mstile-144x144.png" />
